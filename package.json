{
  "name": "next-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker-dev": "docker compose -f compose.dev.yaml up --build",
    "docker-build": "docker compose -f compose.prod.yaml build",
    "docker-start": "docker compose -f compose.prod.yaml up -d",
    "docker-stop": "docker compose -f compose.prod.yaml down",
    "docker-prisma-migrate": "docker exec -it next-blog-app-dev npx prisma migrate",
    "docker-prisma-reset": "docker exec -it next-blog-app-dev npx prisma reset"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.6",
    "@heroicons/react": "^2.1.5",
    "@node-rs/argon2": "^1.8.3",
    "@prisma/client": "^5.19.1",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.6",
    "jsdom": "^25.0.0",
    "marked": "^14.1.2",
    "next": "14.2.9",
    "next-auth": "^5.0.0-beta.21",
    "prisma": "^5.19.1",
    "react": "^18",
    "react-dom": "^18",
    "use-debounce": "^10.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/bcrypt": "^5.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.9",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
